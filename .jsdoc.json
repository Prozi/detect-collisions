{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["source", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|out)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "Collision Detection",
    "disableSort": false
  },
  "opts": {
    "destination": "./out/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc-template"
  }
}
